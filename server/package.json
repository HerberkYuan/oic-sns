{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "migration:up": "ts-node $(npm bin)/typeorm migration:run -f src/infrastructure/config/ormconfig.js",
    "migration:generate": "npx typeorm migration:generate -f src/infrastructure/config/ormconfig.js -n",
    "migration:create": "npx typeorm migration:create -f src/infrastructure/config/ormconfig.js -n",
    "migration:revert": "ts-node $(npm bin)/typeorm migration:revert -f src/infrastructure/config/ormconfig.js",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "concurrently --handle-input \"wait-on dist/src/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/src/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.8.5",
    "@nestjs/core": "^6.8.5",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.8.5",
    "@nestjs/typeorm": "^6.2.0",
    "@types/compression": "^1.0.1",
    "@types/express-rate-limit": "^3.3.3",
    "@types/helmet": "0.0.45",
    "@types/jest": "^23.3.13",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/passport-jwt": "^3.0.2",
    "@types/passport-local": "^1.0.33",
    "cache-manager": "^2.10.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^5.0.0",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.17.1",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "rimraf": "^2.7.1",
    "rxjs": "^6.5.3",
    "ts-node": "8.1.0",
    "typeorm": "^0.2.20",
    "typescript": "3.4.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.11.2",
    "@nestjs/testing": "^6.8.5",
    "@types/express": "^4.17.1",
    "@types/jest": "^23.3.13",
    "@types/mysql": "^2.15.7",
    "@types/node": "^10.17.0",
    "@types/passport": "^1.0.1",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/supertest": "^2.0.7",
    "concurrently": "^4.1.2",
    "jest": "^23.6.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.15.3",
    "supertest": "^3.4.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.4.3",
    "wait-on": "^3.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
